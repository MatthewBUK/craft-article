{
  "custom": {
    "css": [
      "https://cdnjs.cloudflare.com/ajax/libs/tailwindcss/2.2.4/tailwind.min.css"
    ]
  },
  "plugins": [
    "craft-image",
    "craft-image-editor",
    "craft-link",
    "imageposition",
    "reorder",
    "underline",
    "removeformat",
    "selector"
  ],
  "grid": {
    "classname": "grid grid-cols-12",
    "columns": 12,
    "patterns": {
      "6|6": "col-span-6|col-span-6",
      "4|4|4": "col-span-4|col-span-4|col-span-4",
      "3|3|3|3": "col-span-3|col-span-3|col-span-3|col-span-3",
      "2|2|2|2|2|2": "col-span-2|col-span-2|col-span-2|col-span-2|col-span-2|col-span-2",
      "3|6|3": "col-span-3|col-span-6|col-span-3",
      "2|8|2": "col-span-2|col-span-8|col-span-2",
      "5|7": "col-span-5|col-span-7",
      "7|5": "col-span-7|col-span-5",
      "4|8": "col-span-4|col-span-8",
      "8|4": "col-span-8|col-span-4",
      "3|9": "col-span-3|col-span-9",
      "9|3": "col-span-9|col-span-3",
      "2|10": "col-span-2|col-span-10",
      "10|2": "col-span-10|col-span-2",
      "12": "col-span-12"
    }
  }
}